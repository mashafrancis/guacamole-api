{
  "name": "guacamole-api",
  "version": "1.0.0",
  "description": "Guacamole API",
  "main": "src/index.ts",
  "repository": "https://github.com/mashafrancis/guacamole-api.git",
  "author": "Francis Masha <francismasha96@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nodemon --exec ts-node src/index.ts",
    "start:dev": "node -r ts-node/register -r dotenv-safe/config src/index.ts",
    "cli": "node -r dotenv-safe/config -r ts-node/register src/cli.ts",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:unit": "jest -u --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migrations": "typeorm migration:generate -n Migration"
  },
  "dependencies": {
    "@nestjs/common": "^7.5.3",
    "@nestjs/core": "^7.5.3",
    "argon2": "^0.27.0",
    "async-exit-hook": "^2.0.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "dotenv-safe": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "faker": "^5.1.0",
    "googleapis": "^65.0.0",
    "helmet": "^4.2.0",
    "nodemailer": "^6.4.16",
    "passport": "^0.4.1",
    "passport-facebook-token": "^4.0.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "twing": "^5.0.2",
    "typeorm": "^0.2.29",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1",
    "@types/compression": "^1",
    "@types/cors": "^2",
    "@types/dotenv-safe": "^8",
    "@types/errorhandler": "^1",
    "@types/eslint": "^7",
    "@types/eslint-config-prettier": "^6",
    "@types/eslint-plugin-prettier": "^3",
    "@types/express": "^4",
    "@types/faker": "^5",
    "@types/helmet": "^4",
    "@types/nodemailer": "^6",
    "@types/passport": "^0",
    "@types/passport-http-bearer": "^1",
    "@types/prettier": "^2",
    "@types/rimraf": "^3",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  }
}
